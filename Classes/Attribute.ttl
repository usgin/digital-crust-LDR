	rdfs:label "UseApplication"@en ;
	dcdtr:dataType <data-type/string> ;
	dcdtr:meaning <http://resources.usgin.org/uri-gin/dtr/def/property/use-application> .

<attribute/utm-e> a owl:Class ;
