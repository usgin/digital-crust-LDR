<attribute/source> a owl:Class ;
	skos:prefLabel "Source"@en ;
	dct:description "Mandatory. Short text explanation of source of information defining this feature or record; may be author-date type citation, well log, report, data files etc., but should include some indication of how digital data originated. If unpublished data, provide researcher name, date and research institution for proper citation. Separate multiple citations with pipe '|' character." ;
	rdfs:label "Source"@en ;
	dcdtr:dataType <data-type/list-string-pipe> ;
	dcdtr:meaning <http://resources.usgin.org/uri-gin/dtr/def/property/provenance> ;
	dcdtr:valueDomain <value-domain/narrative-text> .

<attribute/srs> a owl:Class ;
	skos:prefLabel "SRS"@en ;
	dct:description "The spatial reference system. It is recommended that an EPSG code be used to identify the SRS used to specify the location.  If an EPSG code is used, identify it as such with the prefix 'EPSG:' For common EPSG codes, see the Data Valid Terms tab. EPSG:4326 is the identifier for WGS84, and should be the text in this field. EPSG stands for European Petroleum Survey Group. EPSG codes can be dereferenced by putting the code in this URL (replace 4326) http://spatialreference.org/ref/epsg/4326/." ;
	rdfs:label "SRS"@en ;
	dcdtr:containerObject <data-object/coordinate-location> ;
	dcdtr:dataType <data-type/string> ;
	dcdtr:meaning <http://resources.usgin.org/uri-gin/dtr/def/property/spatial-reference-system> .

<attribute/state> a owl:Class ;
	skos:prefLabel "State"@en ;
	dct:description "Required. State name without abbreviations. If unknown or not applicable use 'missing'. For offshore locations, specify the governing or nearest state and list the county as 'offshore'." ;
	rdfs:label "State"@en ;
	dcdtr:dataType <data-type/term> ;
	dcdtr:meaning <http://resources.usgin.org/def/property/location-name> ;
	dcdtr:valueDomain <value-domain/us-state-name> .

<attribute/status> a owl:Class ;
	skos:prefLabel "Status"@en ;
	dct:description "Status of the feature at the time indicated in the StatusDate element. If unknown enter \"unknown\" in the field." ;
	rdfs:label "Status"@en ;
	dcdtr:dataType <data-type/string> ;
	dcdtr:meaning <http://resources.usgin.org/uri-gin/dtr/def/property/facility-status> .

<attribute/statusdate> a owl:Class ;
	skos:prefLabel "StatusDate"@en ;
	dct:description "The status of the feature can change from time to time, thus it is useful to know the date on which a given status was reported. Day, month and year must be specified; Excel will display using ISO 8601 date and time (yyyy-mm-ddT:hh:mm) format. If only the year is known, enter month and day as 'Jan. 1', (or '1/1/', or '1-1-', or 'January 1,'). All dates will be converted to yyyy-mm-ddThh:mm." ;
	rdfs:label "StatusDate"@en ;
	dcdtr:dataType <data-type/datetime> ;
	dcdtr:meaning <http://resources.usgin.org/uri-gin/dtr/def/property/reporting-date> ;
	dcdtr:valueDomain <value-domain/era-of-geothermal-drilling> .

<attribute/township> a owl:Class ;
	skos:prefLabel "Township"@en ;
	dct:description "Township in PLSS grid, relative to reported baseline. Formatting and punctuation should be consistent for all locations referenced to the same baseline and meridian. Recommend adding leading '0's so that all number strings are the same length (typically 2 or occasionally 3 digits). Examples 01N, 112S, 001 N." ;
	rdfs:label "Township"@en ;
	dcdtr:containerObject <data-object/us-cadastral-location> ;
	dcdtr:dataType <data-type/string> ;
	dcdtr:meaning <http://resources.usgin.org/uri-gin/dtr/def/property/township-number> .

<attribute/updatetimestamp> a owl:Class ;
