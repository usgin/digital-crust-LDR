	rdfs:label "Related Resource"@en ;
	dcdtr:dataType <data-type/list-string-pipe> ;
	dcdtr:meaning <http://resources.usgin.org/uri-gin/dtr/def/property/related-resource> .

<attribute/plss-section-number> a owl:Class ;
	skos:prefLabel "PLSS section number"@en ;
	dct:description "PLSS section number. Must be numeric or null. Some surveys include half sections indicated by a 'nn.5' designation, where n indicates a number." ;
	rdfs:label "PLSS section number"@en ;
	dcdtr:containerObject <data-object/us-cadastral-location> ;
	dcdtr:dataType <data-type/string> ;
	dcdtr:meaning <http://resources.usgin.org/uri-gin/dtr/def/property/section-number> ;
	dcdtr:valueDomain <value-domain/valid-plss-section-number> .

<attribute/plss-section-part> a owl:Class ;
