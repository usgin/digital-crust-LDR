<object-class/value> a skos:Concept ;
	skos:prefLabel "Value"@en ;
	dct:description "an entity that represents a primitive value" ;
	dc:source "SMR analysis" ;
	dcdtr:type "information object" ;
	rdfs:label "Value"@en .

<object-class/agent> a skos:Concept ;
	skos:prefLabel "Agent"@en ;
	dct:description "An entity that bears some form of responsibility for an activity taking place, for the existence of an entity, or for another agent's activity." ;
	dc:source "https://www.w3.org/TR/prov-o/#Agent" ;
	rdfs:label "Agent"@en .

<object-class/abandoned-underground-mine-feature> a skos:Concept ;
	skos:prefLabel "Abandoned Underground Mine Feature"@en ;
	skos:altLabel "Abandoned Mines"@en ;
	dc:source "NGDS" ;
	dcdtr:type "feature" ;
	rdfs:label "Abandoned Underground Mine Feature"@en .

<object-class/borehole-casing> a skos:Concept ;
	skos:prefLabel "Borehole Casing"@en ;
	dct:description "Characterization of casing pipe used for borehole engineering. " ;
	skos:altLabel "Borehole Casing"@en ;
	dc:source "SMR analysis" ;
	dcdtr:type "information object" ;
	rdfs:label "Borehole Casing"@en .

<object-class/borehole-collar> a skos:Concept ;
	skos:prefLabel "Borehole Collar"@en ;
	dct:description "The location at which a borehole trace originates. Might be within a well, along an existing borehole trace, or in the subsurface, e.g. collared in a mine tunnel" ;
	dc:source "SMR analysis" ;
	dcdtr:type "feature" ;
	rdfs:label "Borehole Collar"@en .

<object-class/borehole-intercept> a skos:Concept ;
	skos:prefLabel "Borehole Intercept"@en ;
	dct:description "A sampling feature  whose feature of interest is a site at a particular single depth in a borehole." ;
	skos:altLabel "Borehole Lithology Intercept"@en ;
	dc:source "NGDS" ;
	dcdtr:type "sampling feature" ;
	rdfs:label "Borehole Intercept"@en .

<object-class/borehole-interval> a skos:Concept ;
	skos:prefLabel "Borehole Interval"@en ;
	dct:description "A sampling feature  whose feature of interest is the interval of material penetrated by a borehole between two specified depths." ;
	skos:altLabel "Borehole Lithology Interval"@en ;
	dc:source "NGDS" ;
	dcdtr:type "sampling feature" ;
	rdfs:label "Borehole Interval"@en .

<object-class/borehole-temperature-observation> a skos:Concept ;
	skos:prefLabel "Borehole Temperature Observation"@en ;
	dct:description "subsurface temperature measurement made in a borehole" ;
	skos:altLabel "Borehole Temperature Observation"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Borehole Temperature Observation"@en .

<object-class/event> a skos:Concept ;
	skos:prefLabel "Event"@en ;
	dct:description "A entity that has a temporal extent or position, and an associated process or activity. " ;
	dc:source "SMR analysis" ;
	dcdtr:type "temporal object" ;
	rdfs:label "Event"@en .

<object-class/geologic-contact-feature> a skos:Concept ;
	skos:prefLabel "Geologic Contact Feature"@en ;
	dct:description "A mapped feature that represents the outcrop trace of a geologic boundary between two geologic units that is not a fault " ;
	skos:altLabel "Geologic Contact Feature"@en ;
	dc:source "NGDS" ;
	dcdtr:type "mapped feature" ;
	rdfs:label "Geologic Contact Feature"@en .

<object-class/geologic-fault-feature> a skos:Concept ;
	skos:prefLabel "Geologic Fault Feature"@en ;
	dct:description "A mapped feature that represents the outcrop trace of a geologic fault. " ;
	skos:altLabel "Shear Displacement Structure"@en ;
	dc:source "NGDS" ;
	dcdtr:type "mapped feature" ;
	rdfs:label "Geologic Fault Feature"@en .

<object-class/geologic-reservoir-feature> a skos:Concept ;
	skos:prefLabel "Geologic Reservoir Feature"@en ;
	dct:description "a subsurface body of material that contains fluids of interest that can be extracted." ;
	skos:altLabel "Geologic Reservoir"@en ;
	dc:source "NGDS" ;
	dcdtr:type "geologic feature" ;
	rdfs:label "Geologic Reservoir Feature"@en .

<object-class/geologic-unit-feature> a skos:Concept ;
	skos:prefLabel "Geologic Unit Feature"@en ;
	dct:description "A body of material in the Earth whose complete and precise extent is inferred to exist (NADM GeologicUnit, Stratigraphic unit in sense of NACSN or International Stratigraphic Code), or a classifier used to characterize parts of the Earth (e.g. lithologic map unit like 'granitic rock' or 'alluvial deposit', surficial units like 'till' or 'old alluvium').  Includes both formal units (i.e. formally adopted and named in the official lexicon) and informal units (i.e. named but not promoted to the lexicon) and unnamed units (i.e. recognisable and described and delineable in the field but not otherwise formalised)." ;
	skos:altLabel "Geologic Unit"@en ;
	dc:source "http://www.geosciml.org/geosciml/4.0/documentation/html/" ;
	dcdtr:type "geologic feature" ;
	rdfs:label "Geologic Unit Feature"@en .

<object-class/geologic-unit-outcrop> a skos:Concept ;
	skos:prefLabel "Geologic Unit Outcrop"@en ;
	dct:description "A mapped feature that represents the outcrop of a GeologicUnit on some map horizon." ;
	dcdtr:type "mapped feature" ;
	rdfs:label "Geologic Unit Outcrop"@en .

<object-class/geothermal-area-feature> a skos:Concept ;
	skos:prefLabel "Geothermal Area Feature"@en ;
	dct:description "A region on the surface of the Earth characterized by the presence of features manifesting geothermal activity" ;
	skos:altLabel "Geothermal Area"@en ;
	dc:source "NGDS" ;
	dcdtr:type "mapped feature" ;
	rdfs:label "Geothermal Area Feature"@en .

<object-class/geothermal-direct-use-facility> a skos:Concept ;
	skos:prefLabel "Geothermal Direct Use Facility"@en ;
	dct:description "basic description of facilities that utilize geothermal energy directly without transformation to electricity" ;
	skos:altLabel "Direct Use Facility"@en ;
	dc:source "NGDS" ;
	dcdtr:type "facility" ;
	rdfs:label "Geothermal Direct Use Facility"@en .

<object-class/geothermal-power-plant-facility> a skos:Concept ;
	skos:prefLabel "Geothermal Power Plant Facility"@en ;
	dct:description "A facility that generates electricity using geothermal energy." ;
	skos:altLabel "Geothermal Power Plant Facility"@en ;
	dc:source "NGDS" ;
	dcdtr:type "facility" ;
	rdfs:label "Geothermal Power Plant Facility"@en .

<object-class/gravity-observation> a skos:Concept ;
	skos:prefLabel "Gravity Observation"@en ;
	dct:description "A site sampling feature specifically for a gravity observation" ;
	skos:altLabel "Gravity Station"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Gravity Observation"@en .

<object-class/heat-flow-observation> a skos:Concept ;
	skos:prefLabel "Heat Flow Observation"@en ;
	dct:description "description of heat flow measurements" ;
	skos:altLabel "Heat Flow Observation"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Heat Flow Observation"@en .

<object-class/heat-pump-facility> a skos:Concept ;
	skos:prefLabel "Heat Pump Facility"@en ;
	dct:description "description of heat pump facility locations" ;
	skos:altLabel "Heat Pump Facility"@en ;
	dc:source "NGDS" ;
	dcdtr:type "facility" ;
	rdfs:label "Heat Pump Facility"@en .

<object-class/hydraulic-properties-observation> a skos:Concept ;
	skos:prefLabel "Hydraulic Properties Observation"@en ;
	dct:description "A specification of the hydraulic properties of some sampling feature." ;
	skos:altLabel "Hydraulic Properties Observation"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Hydraulic Properties Observation"@en .

<object-class/isopleth-feature> a skos:Concept ;
	skos:prefLabel "Isopleth Feature"@en ;
	dct:description "a connected sequence of points characterized by equal values for some observed property" ;
	skos:altLabel "Contour Line Feature"@en ;
	dc:source "NGDS" ;
	dcdtr:type "feature" ;
	rdfs:label "Isopleth Feature"@en .

<object-class/mineral-recovery-brine-experimental-data> a skos:Concept ;
	skos:prefLabel "Mineral Recovery Brine Experimental Data"@en ;
	dct:description "Record of experimental parameters related to experimental recovery of products from brines.  capturing the variables to describe/evaluate REE removal and capturing the variables influencing removal and describing removal media; Essentially metadata for experimental configuration. Use to record observations of REE extraction processes and can be used to compare the efficacy of those processes." ;
	skos:altLabel "Brin Mineral Recovery Experimental Data"@en ;
	dc:source "NGDS" ;
	dcdtr:type "information object" ;
	rdfs:label "Mineral Recovery Brine Experimental Data"@en .

<object-class/mineral-recovery-brine-process-and-economics> a skos:Concept ;
	skos:prefLabel "Mineral Recovery Brine Process And Economics"@en ;
	dct:description "Record of  parameters in the product removal (“reactor”) technique used to recover minerals from a brine, the ability of the media to be re-used, and the techno-economic and process economics. Use to record observations of REE extraction processes and can be used to compare the efficacy of those processes." ;
	skos:altLabel "Brin Mineral Recovery Experimental Data"@en ;
	dc:source "NGDS" ;
	dcdtr:type "information object" ;
	rdfs:label "Mineral Recovery Brine Process And Economics"@en .

<object-class/physical-sample> a skos:Concept ;
	skos:prefLabel "Physical Sample"@en ;
	dct:description "An object extracted from a larger feature with the intention of representing one or more characteristics of that feature." ;
	skos:altLabel "Physical Sample"@en ;
	dc:source "NGDS" ;
	dcdtr:type "sampling feature" ;
	rdfs:label "Physical Sample"@en .

<object-class/powell-and-cumming-geothermometry-observation> a skos:Concept ;
	skos:prefLabel "Powell And Cumming Geothermometry Observation"@en ;
	dct:description "Observation results requires to run Powell and Cumming geothermometry calculations for geothermal fluids" ;
	skos:altLabel "Powell And Cumming Geothermometry"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Powell And Cumming Geothermometry Observation"@en .

<object-class/power-plant-production-observation> a skos:Concept ;
	skos:prefLabel "Power Plant Production Observation"@en ;
	dct:description "Observation with a result reporting the  electrical power production for a specific time interval from a power generating facility" ;
	skos:altLabel "Power Plant Production"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Power Plant Production Observation"@en .

<object-class/radiogenic-heat-production-observation> a skos:Concept ;
	skos:prefLabel "Radiogenic Heat Production Observation"@en ;
	dct:description "Observation with a  result reporting the heat energy production rate from a sampling feature,  typically based on analysis of ore or more individual rock samples" ;
	skos:altLabel "Radiogenic Heat Production"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Radiogenic Heat Production Observation"@en .

<object-class/resource-documentation> a skos:Concept ;
	skos:prefLabel "Resource Documentation"@en ;
	dct:description "Collection of properties to describe and existing resource, including information for discovery, evaluation and access." ;
	skos:altLabel "Metadata"@en ;
	dc:source "NGDS" ;
	dcdtr:type "information object" ;
	rdfs:label "Resource Documentation"@en .

<object-class/rock-chemical-composition-observation> a skos:Concept ;
	skos:prefLabel "Rock Chemical Composition Observation"@en ;
	dct:description "observation whose result is the concentration of one or more analytes in a sampling feature whose material composition is a solid EarthMaterial." ;
	skos:altLabel "Rock Chemistry"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Rock Chemical Composition Observation"@en .

<object-class/seismic-event-hypocenter-observation> a skos:Concept ;
	skos:prefLabel "Seismic Event Hypocenter Observation"@en ;
	dct:description "Observation with a result that is the location of the hypocenter of a seismic event" ;
	skos:altLabel "Seismic Event Hypocenter"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Seismic Event Hypocenter Observation"@en .

<object-class/thermal-conductivity-observation> a skos:Concept ;
	skos:prefLabel "Thermal Conductivity Observation"@en ;
	dct:description "Observation with a result that is the thermal conductivity of a sampling feature." ;
	skos:altLabel "Thermal Conductivity Observation"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Thermal Conductivity Observation"@en .

<object-class/thermal-spring> a skos:Concept ;
	skos:prefLabel "Thermal Spring"@en ;
	dct:description "characterizing a hot spring feature" ;
	skos:altLabel "Thermal Spring"@en ;
	dc:source "NGDS" ;
	dcdtr:type "hydrologic feature" ;
	rdfs:label "Thermal Spring"@en .

<object-class/volcanic-vent-feature> a skos:Concept ;
	skos:prefLabel "Volcanic Vent Feature"@en ;
	dct:description "A feature in the Earth's surface from which lava has been erupted." ;
	skos:altLabel "Volcanic Vent"@en ;
	dc:source "NGDS" ;
	dcdtr:type "geologic feature" ;
	rdfs:label "Volcanic Vent Feature"@en .

<object-class/well> a skos:Concept ;
	skos:prefLabel "Well"@en ;
	dct:description "A facility intended to provide access to the subsurface of the Earth; might be used for extraction or injection of materials; has one or more borehole traces and a collar location" ;
	dcdtr:type "feature" ;
	rdfs:label "Well"@en .

<object-class/well-fluid-flux-observation> a skos:Concept ;
	skos:prefLabel "Well Fluid Flux Observation"@en ;
	dct:description "Observation whose result is the fluid flux into or out of a well." ;
	skos:altLabel "Fluid Flux Injection And Disposal Observation"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Well Fluid Flux Observation"@en .

<object-class/well-fluid-production-observation> a skos:Concept ;
	skos:prefLabel "Well Fluid Production Observation"@en ;
	dct:description "Observation with a result that specifies the volume of fluid produced from a given well. A generic model is intended to allow for a variety of flows from a well including oil, gas, and water as well as steam production. " ;
	skos:altLabel "Well Fluid Production"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Well Fluid Production Observation"@en .

<object-class/well-header-feature> a skos:Concept ;
	skos:prefLabel "Well Header Feature"@en ;
	dct:description "Feature that is the intersection of a well with a mapping horizon. In a simple well, the well header is identical with the borehole collar for a single borehole trace that constitutes the well." ;
	skos:altLabel "Well Header Observation"@en ;
	dc:source "NGDS" ;
	dcdtr:type "feature" ;
	rdfs:label "Well Header Feature"@en .

<object-class/well-log-observation> a skos:Concept ;
	skos:prefLabel "Well Log Observation"@en ;
	dct:description "An observation with a result that is a coverage of property values at intervals along the trace of a borehole" ;
	skos:altLabel "Well Log Observation"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Well Log Observation"@en .

<object-class/well-test-observation> a skos:Concept ;
	skos:prefLabel "Well Test Observation"@en ;
	dct:description "Observation with a result that consists of fluid flux, pressure and temperature data; includes inforamtion charactrizing the experimental conditions" ;
	skos:altLabel "Well Test"@en ;
	dc:source "NGDS" ;
	dcdtr:type "observation" ;
	rdfs:label "Well Test Observation"@en .

<object-class/geospatial-position> a skos:Concept ;
	skos:prefLabel "Geospatial Position"@en ;
	dct:description "A point location on the Earth" ;
	rdfs:label "Geospatial Position"@en .