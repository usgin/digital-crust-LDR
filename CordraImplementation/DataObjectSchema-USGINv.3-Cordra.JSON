                 "type": {
                     "autoGeneratedField": "createdBy"
                 }
             }
         },
         "modifiedBy": {
             "type": "string",
             "title": "Modified By",
             "net.cnri.repository": {
                 "type": {
                     "autoGeneratedField": "modifiedBy"
                 }
             }
         }
     }
 }