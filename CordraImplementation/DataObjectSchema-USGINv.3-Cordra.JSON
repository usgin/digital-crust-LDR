 {
     "type": "object",
     "title": "DataObject",
     "description": "Schema for registry item that defines a Data Object subtype of DataType. The data object has 1 or more attributes, each with its own data type. Some properties in the UML model are not implemented: purpose, intendedUse, and logicType. Version 2017-04-25; Author: SM Richard",
     "@context": {
         "dt": "http://resources.usgin.org/uri-gin/usgin/schema/json/0.3/datatype",
         "@vocab": "http://json-schema.org/draft-04/schema",
         "@base": "http://resources.usgin.org/uri-gin/usgin/schema/json/0.3/datatype",
         "foaf": "http://xmlns.com/foaf/0.1/",
         "dct": "http://purl.org/dc/terms/",
         "skos": "http://www.w3.org/2004/02/skos/core#"
     },
     "properties": {
         "identifier": {
             "type": "string",
             "net.cnri.repository": {
                 "type": {
                     "autoGeneratedField": "handle"
                 }
             }
         },
         "prefLabel": {
             "type": "string",
             "title": "Name",
             "description": "Name for this data object, property inherited from dataType but aliased here to avoid confusion. DataObject is a subtype of the more general, abstract DataType.Implements dt:dataObjectName",
             "net.cnri.repository": {
                 "preview": {
                     "showInPreview": true,
                     "isPrimary": true
                 }
             }
         },
         "alternateLabels": {
             "type": "array",
             "title": "other designations for the data object",
             "description": "alternate names for labeling the entity in UI; name could be localized with an @ extension eg. tamanho@PT. Implements dt:localNames",
             "items": {
                 "type": "object",
                 "properties": {
                     "altLabel": {
                         "type": "string"
                     },
                     "altLabelContext": {
                         "type": "string"
                     }
                 }
             }
         },
         "description": {
             "type": "string",
             "format": "textarea",
             "title": "Definition",
             "net.cnri.repository": {
                 "preview": {
                     "showInPreview": true,
                     "excludeTitle": true
                 }
             }
         },
         "source": {
             "type": "string",
             "format": "textarea",
             "title": "Source of definition",
             "net.cnri.repository": {
                 "preview": {
                     "showInPreview": false,
                     "excludeTitle": true
                 }
             }
         },
         "dt:meaning": {
             "description": "concept label and identifier that specifies the semantics of the thing represented by instances of this dataType",
             "type": "object",
             "title": "DataType semantics",
             "properties": {
                 "dt:objectMeaningLabel": {
                     "description": "text label for people to recognize what the object represents",
                     "type": "string",
                     "title": "Data Object Concept"
                 },
                 "dt:objectMeaningURI": {
                     "description": "identifier for a data type (objectClass) entity concept",
                     "title": "Concept URI",
                     "anyOf": [
                         {
                             "type": "string",
                             "title": "External URI",
                             "format": "URI"
                        },
                         {
                             "type": "string",
                             "title": "Object Class in this repository: ",
                             "net.cnri.repository": {
                                 "type": {
                                     "handleReference": {
                                         "types": ["Object Class"]
                                     }
                                 }
                             }
                            }
                        ]
                 }
             }
         },
         "dt:elements": {
             "description": "basic properties to characterize attributes included in an entity content model. JSON $ref key can be used to link definition to a registered attribute definition. The propertySpecified property is used for semantic integration",
             "title": "Attributes",
             "type": "array",
             "minItems": 1,
             "items": {
                 "type": "string",
                 "title": "Attribute link",
                 "net.cnri.repository": {
                     "type": {
                         "handleReference": {
                             "types": ["Attribute"]
                         }
                     }
                 }
             }
         },
         "dt:metaAttributes": {
             "description": "other attributes of dataObject instances. these are Key-value pairs. The keys are foreign keys (links) to attribute elements.",
             "type": "object",
             "properties": {
                 "metaAttributeValue": {
                     "type": "string"
                 },
                 "metadataAttributeValueURI": {
                     "description": "Identifier for the attribute value; should be thought of as a link, identifier should be dereferenceable to get content",
                     "type": "string",
                     "format": "URI"

                 },
                 "metaElement": {
                     "description": "identifier for an attribute entity that specifies the property",
                     "type": "string",
                     "title": "meta Attributre",
                     "net.cnri.repository": {
                         "type": {
                             "handleReference": {
                                 "types": ["Attribute"]
                             }
                         }
                     }
                 }
             }
         },
         "creationDate": {
